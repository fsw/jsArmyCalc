<?xml version="1.0" encoding="utf-8"?>
<ruleset xmlns="armycalc" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="armycalc http://armycalc.com/xmls/twr2info.xsd" version="2.0">
<uid>ac_example_ruleset</uid>
<revision>1</revision>
<name>Example Ruleset</name>
<author>francio</author>
<description>This is an example Ruleset.
It contains example units, add-ons and validation script for the abstract game system.</description>
<defaultArmyName>Army</defaultArmyName>
<defaultArmySize>1500</defaultArmySize>
<displayFields>
    <field>
      <name>dexterity</name>
      <default>3</default>
    </field>
    <field>
      <name>strength</name>
      <default>3</default>
    </field>
    <field>
      <name>agility</name>
      <default>3</default>
    </field>
    <field>
      <name>lives</name>
      <default>1</default>
    </field>
  </displayFields>
<files>
<units>
<file><name></name><path>units.xml</path></file>
</units>
<validators>
<file><name></name><path>validator.lua</path></file>
</validators>
<displays>
</displays>
<media>media</media>
</files>
</ruleset>